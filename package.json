        "better-sqlite3": "^9.2.2",
        "eslint": "^8",
        "eslint-config-next": "14.0.4",
        "gh-pages": "^6.1.1",
        "husky": "^8.0.3",
        "lint-staged": "^15.2.0",
        "prettier": "^3.1.1",
        "typescript": "^5"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
